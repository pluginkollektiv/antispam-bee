{
  "extends": "plugin:@wordpress/eslint-plugin/es5",
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "Chartist": "readonly",
    "getAnchors": "readonly"
  },
  "rules": {
    "camelcase": 0,
    "eqeqeq": 0,
    "no-mixed-operators": 0,
    "no-redeclare": 0,
    "no-shadow": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 0,
    "vars-on-top": 0
  }
}
